```python
â€# ai_tagging.py
â€
â€import cv2
â€import numpy as np
â€
â€def tag_video(video_path):
â€Â Â Â  # Load pre-trained object detection model (e.g., YOLOv3 or MobileNet)
â€Â Â Â  net = cv2.dnn.readNet("yolov3.weights", "yolov3.cfg")
â€Â Â Â  layer_names = net.getUnconnectedOutLayersNames()
â€
â€Â Â Â  cap = cv2.VideoCapture(video_path)
â€Â Â Â  detected_objects = set()
â€
â€Â Â Â  while cap.isOpened():
â€Â Â Â Â Â Â Â  ret, frame = cap.read()
â€Â Â Â Â Â Â Â  if not ret:
â€Â Â Â Â Â Â Â Â Â Â Â  break
â€
â€Â Â Â Â Â Â Â  blob = cv2.dnn.blobFromImage(frame, 1/255.0, (416, 416), swapRB=True, crop=False)
â€Â Â Â Â Â Â Â  net.setInput(blob)
â€Â Â Â Â Â Â Â  outputs = net.forward(layer_names)
â€
â€Â Â Â Â Â Â Â  for output in outputs:
â€Â Â Â Â Â Â Â Â Â Â Â  for detection in output:
â€Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  scores = detection[5:]
â€Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  class_id = np.argmax(scores)
â€Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  confidence = scores[class_id]
â€Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if confidence> 0.5:
â€Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  detected_objects.add(str(class_id))Â  # Replace with class name mapping
â€
â€Â Â Â  cap.release()
â€Â Â Â  return list(detected_objects)
â€
â€# Example usage
â€if __name__ == "__main__":
â€Â Â Â  tags = tag_video("sample_video.mp4")
â€Â Â Â  print("Detected objects:", tags)
â€```
